Names Default To Here( 1 );dt = Current Data Table();col_list = dt << Get Selected Columns( string );For( i = 1, i <= N Items( col_list ), i++,	Eval(		Substitute(				Expr(					Column(col_list[i])  << Data Type( Character );					dt << Add Multiple Columns( __NewColName__, 1, Numeric );					Column( __NewColName__ ) << set formula( If( is missing( __OrigCol__), 5 , contains( __OrigCol__, "<"), 0 , 1 ) );				),			Expr( __NewColName__ ), "D_" || col_list[i],			Expr( __OrigCol__ ), Parse( ":Name(\!"" || col_list[i] || "\!")" )		)	)	);		